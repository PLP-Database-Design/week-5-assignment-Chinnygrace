<table>
  <thead>
    <tr>
      <th>First Name</th>
      <th>Last Name</th>
      <th>PATIENT ID</th>
      <th>DATE OF BIRTH</th>
    </tr>
  </thead>
  <tbody>
    <% data.forEach(row =>{%>
    <tr>
      <td><%= row.first_name %></td>
      <td><%= row.last_name %></td>
      <td><%= row.patient_id %></td>
      <td><%= row.date_of_birth %></td>
    </tr>
    <%});%>
  </tbody>
</table>
